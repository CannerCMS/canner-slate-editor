{
  "name": "slate-md-editor",
  "version": "0.3.0",
  "description": "Add live markdown preview and edit to your Slate editor.",
  "repository": {
    "type": "git",
    "url": "https://github.com/canner/slate-md-editor.git"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "prettier --write ./src/**/*.js ./docs/*.js && eslint src test docs/*.js",
    "clean": "rimraf lib dist",
    "build:flow": "flow-copy-source -v -i '**/test/**' src lib",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "npm run build:commonjs && npm run build:flow",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "check:src": "npm run lint",
    "test": "cross-env BABEL_ENV=test karma start --single-run",
    "test:watch": "cross-env BABEL_ENV=test karma start",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --mode development",
    "build:docs": "./node_modules/.bin/webpack --config webpack.config.prod.js --mode production",
    "postpublish": "npm run build:docs",
    "validate": "npm ls"
  },
  "devDependencies": {
    "antd": "^3.3.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "flow-copy-source": "^1.3.0",
    "husky": "^0.13.3",
    "immutable": "^4.0.0-rc.9",
    "jest": "22.4.2",
    "js-beautify": "^1.7.5",
    "less": "2.3.1",
    "less-loader": "^4.1.0",
    "prettier": "^1.9.2",
    "prism": "^4.1.2",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.1",
    "slate": "^0.33.0",
    "slate-plain-serializer": "^0.5.9",
    "slate-react": "^0.12.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || 16.x",
    "react-dom": "^0.14.0 || ^15.0.0 || 16.x",
    "slate": ">=0.33.0"
  },
  "dependencies": {
    "@canner/slate-icon-blockquote": "^1.2.5",
    "@canner/slate-icon-bold": "^1.2.5",
    "@canner/slate-icon-code": "^1.2.5",
    "@canner/slate-icon-codeblock": "^1.2.5",
    "@canner/slate-icon-header": "^1.2.5",
    "@canner/slate-icon-hr": "^1.2.5",
    "@canner/slate-icon-image": "^1.2.5",
    "@canner/slate-icon-italic": "^1.2.5",
    "@canner/slate-icon-link": "^1.2.5",
    "@canner/slate-icon-list": "^1.2.5",
    "@canner/slate-icon-shared": "^1.2.5",
    "@canner/slate-icon-strikethrough": "^1.2.5",
    "@canner/slate-icon-underline": "^1.2.5",
    "@canner/slate-helper-block-list": "^0.1.12",
    "@canner/slate-helper-mark-removeall": "^0.1.3",
    "github-markdown-css": "^2.10.0",
    "markup-it": "^7.0.0",
    "prismjs": "^1.13.0",
    "slate-edit-blockquote": "^0.6.0",
    "slate-edit-code": "^0.15.0",
    "slate-edit-list": "^0.11.2",
    "slate-prism": "^0.4.0",
    "slate-soft-break": "^0.6.0",
    "styled-components": "^3.2.1"
  },
  "keywords": [
    "slate",
    "plugin",
    "markdown"
  ]
}
